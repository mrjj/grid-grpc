{
  "name": "grid-grpc",
  "version": "1.6.4",
  "author": {
    "name": "Ilya Kutukov",
    "email": "post.ilya@gmail.com"
  },
  "description": "GRPC Grid service manager",
  "license": "Unlicense",
  "main": "./src/",
  "bin": {
    "grid-grpc": "bin/grid-grpc"
  },
  "directories": {
    "protos": "protos"
  },
  "files": [
    "src/",
    "example/",
    "bin/grid-grpc"
  ],
  "scripts": {
    "build": "bin/grid-grpc gen -q ./example/echo.proto ./example/public/dist/EchoServiceClient.js",
    "ci": "npm run build && npm run lint && npm run test",
    "dev": "npm run make:client:web ; node ./example/server.js",
    "lint": "eslint src",
    "test": "jest"
  },
  "dependencies": {
    "@grpc/proto-loader": "latest",
    "express": "4.16.4",
    "google-protobuf": "3.6.1",
    "grpc": "1.15.1",
    "helmet": "3.14.0",
    "http-proxy-middleware": "0.19.0",
    "jsonwebtoken": "^8.3.0",
    "lodash.get": "4.4.2",
    "lodash.set": "4.3.2",
    "mustache": "3.0.0",
    "pify": "4.0.1",
    "pino": "5.8.0",
    "pino-pretty": "2.2.3",
    "protobufjs": "6.8.8",
    "ws": "6.1.1",
    "yargs": "12.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-import": "^1.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-babel": "5.2.1",
    "eslint-plugin-eslint-comments": "^2.0.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jasmine": "2.10.1",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "jest": "23.6.0"
  }
}
